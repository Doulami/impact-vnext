{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "npm run clean && concurrently npm:dev:*",
    "clean": "rm -rf dist admin-ui",
    "migrate": "ts-node ./run-migration.ts",
    "seed:bundles": "ts-node ./scripts/seed-demo-bundles.ts",
    "seed:products": "ts-node ./scripts/seed-demo-products.ts",
    "seed:internal": "ts-node ./scripts/seed-internal.ts",
    "build": "tsc",
    "compile:admin-ui": "ts-node compile-admin-ui.ts",
    "dashboard:dev": "vite",
    "dashboard:build": "vite build",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*"
  },
  "dependencies": {
    "@angular/common": "^19.2.15",
    "@angular/compiler": "^19.2.15",
    "@angular/core": "^19.2.15",
    "@angular/forms": "^19.2.15",
    "@angular/platform-browser": "^19.2.15",
    "@angular/platform-browser-dynamic": "^19.2.15",
    "@angular/router": "^19.2.15",
    "@elastic/elasticsearch": "^9.2.0",
    "@nestjs/schedule": "^6.0.1",
    "@pinelab/vendure-plugin-admin-ui-helpers": "^1.4.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vendure/admin-ui-plugin": "3.5.0",
    "@vendure/asset-server-plugin": "3.5.0",
    "@vendure/core": "3.5.0",
    "@vendure/dashboard": "3.5.0",
    "@vendure/elasticsearch-plugin": "^3.5.0",
    "@vendure/email-plugin": "3.5.0",
    "@vendure/graphiql-plugin": "3.5.0",
    "@vendure/ui-devkit": "3.5.0",
    "axios": "^1.7.9",
    "dotenv": "17.2.3",
    "pg": "8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@vendure/cli": "3.5.0",
    "concurrently": "9.2.1",
    "typescript": "5.8.2",
    "vite": "6.4.1"
  }
