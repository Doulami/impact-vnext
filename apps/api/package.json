{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "npm run clean && concurrently npm:dev:*",
    "clean": "rm -rf dist",
    "migrate": "ts-node ./run-migration.ts",
    "seed:bundles": "ts-node ./scripts/seed-demo-bundles.ts",
    "seed:products": "ts-node ./scripts/seed-demo-products.ts",
    "seed:internal": "ts-node ./scripts/seed-internal.ts",
    "build": "tsc",
    "dashboard:dev": "vite",
    "dashboard:build": "vite build",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.2.0",
    "@nestjs/schedule": "^6.0.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vendure/asset-server-plugin": "3.5.1",
    "@vendure/core": "3.5.1",
    "@vendure/dashboard": "3.5.1",
    "@vendure/elasticsearch-plugin": "^3.5.1",
    "@vendure/email-plugin": "3.5.1",
    "@vendure/graphiql-plugin": "3.5.1",
    "axios": "^1.7.9",
    "dotenv": "17.2.3",
    "pg": "8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vendure/cli": "3.5.1",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "9.2.1",
    "typescript": "5.8.2",
    "vite": "6.4.1"
  },
  "volta": {
    "node": "22.21.1"
  }
}
