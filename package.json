{
  "name": "impact-nutrition",
  "version": "1.0.0",
  "description": "Impact Nutrition E-commerce Platform - Monorepo with Next.js frontend and Strapi CMS",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:blog\" \"npm run dev:web\"",
    "dev:web": "cd apps/web && npm run dev",
    "dev:blog": "cd apps/blog && npm run develop",
    "build": "npm run build:web && npm run build:blog",
    "build:web": "cd apps/web && npm run build",
    "build:blog": "cd apps/blog && npm run build",
    "start": "npm run start:web && npm run start:blog",
    "start:web": "cd apps/web && npm run start",
    "start:blog": "cd apps/blog && npm run start",
    "lint": "npm run lint:web && npm run lint:blog",
    "lint:web": "cd apps/web && npm run lint",
    "lint:blog": "echo 'No lint script for Blog'",
    "clean": "npm run clean:deps && npm run clean:build",
    "clean:deps": "rm -rf node_modules apps/*/node_modules packages/*/node_modules",
    "clean:build": "rm -rf apps/web/.next apps/blog/build apps/blog/.tmp",
    "install:all": "npm install && npm run install:apps",
    "install:apps": "cd apps/web && npm install && cd ../blog && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "rxjs": "^7.8.1"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Doulami/impact-vnext.git"
  },
  "keywords": [
    "ecommerce",
    "nextjs",
    "strapi",
    "typescript",
    "tailwindcss",
    "graphql",
    "monorepo"
  ],
  "author": "Impact Nutrition Team",
  "license": "MIT",
  "volta": {
    "node": "22.21.1",
    "npm": "10.9.4"
  },
  "overrides": {
    "@vendure/dashboard": "3.5.0",
    "@vitejs/plugin-react-swc": "4.2.0",
    "@swc/core": "1.15.0"
  }
}
